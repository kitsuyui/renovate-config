{
  "extends": [
    "config:base",
    ":dependencyDashboard",
    "default:pinDigestsDisabled"
  ],
  "pin": {
    "automerge": true
  },
  "cargo": {
    "packageRules": [
      {
        "groupName": "serde",
        "matchPackageNames": [
          "serde",
          "serde_derive",
          "serde_json",
          "serde_yaml"
        ],
        "matchPackagePatterns": [
          "^serde"
        ]
      }
    ]
  }
}