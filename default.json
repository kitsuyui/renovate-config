{
  "extends": [
    "config:base",
    ":dependencyDashboard",
    "default:pinDigestsDisabled"
  ],
  "pin": {
    "automerge": true
  },
  "lockFileMaintenance": {
    "enabled": true
  },
  "cargo": {
    "packageRules": [
      {
        "groupName": "serde",
        "matchPackageNames": [
          "serde",
          "serde_derive",
          "serde_json",
          "serde_yaml"
        ],
        "matchPackagePatterns": [
          "^serde"
        ]
      },
      {
        "groupName": "pyo3",
        "matchPackageNames": [
          "pyo3",
          "pyo3-build-config",
          "pyo3-ffi",
          "pyo3-macros",
          "pyo3-macros-backend"
        ]
      }
    ]
  },
  "python": {
    "packageRules": [
      {
        "groupName": "boto3",
        "matchPackageNames": [
          "boto3",
          "botocore",
          "boto3-stubs"
        ]
      }
    ]
  }
}